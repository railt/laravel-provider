includes:
  - vendor/phpat/phpat/extension.neon

parameters:
  level: 1
  tmpDir: vendor/.phpstan.cache
  paths:
    - libs
  excludePaths:
    - libs/*/tests/*
  phpat:
    ignore_built_in_classes: true

services:
  - class: Railt\LaravelProvider\Architecture\ComposerDependenciesTest
    tags: [ phpat.test ]
